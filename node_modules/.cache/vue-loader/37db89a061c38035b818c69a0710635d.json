/el-input>\n        </el-row>\n        <el-row>\n          <el-button type=\"primary\" round @click=\"login\">登录</el-button>\n        </el-row>\n      </div>\n    </el-card>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"Index\",\n  data() {\n    return {\n      username: '',\n      password: '',\n    }\n  },\n  methods:{\n    login(){\n      let that = this\n      let data = new FormData()\n      data.append('username',that.username)\n      data.append('password', that.password)\n      that.axios.post('/sys/login/restful',data)\n        .then(response => {\n          that.$message('登陆成功')\n          localStorage.setItem('token', response.data.token)\n          that.$router.push({path: '/home'})\n        })\n        .catch(error => {\n          console.log(error)\n        })\n    }\n  }\n}\n</script>\n\n<style scoped>\n.out-div {\n  position: relative;\n}\n\n.in-card {\n  top: 50%;\n  margin: 150px auto;\n}\n\n.row{\n  margin-bottom: 20px;\n}\n\n</style>\n"]}]}