</el-button>\n\t\t\t\t</el-row>\n\t\t\t</div>\n\t\t</el-card>\n\t</div>\n</template>\n\n<script>\n\texport default {\n\t\tname: \"Index\",\n\t\tdata() {\n\t\t\treturn {\n\t\t\t\tusername: '',\n\t\t\t\tpassword: '',\n\t\t\t}\n\t\t},\n\t\tmethods: {\n\t\t\tlogin() {\n\t\t\t\tlet that = this\n\t\t\t\tlet data = new FormData()\n\t\t\t\tdata.append('username', that.username)\n\t\t\t\tdata.append('password', that.password)\n\t\t\t\tthat.axios.post('/sys/login/restful', data)\n\t\t\t\t\t.then(response => {\n\t\t\t\t\t\tthat.$message('登陆成功')\n\t\t\t\t\t\tlocalStorage.setItem('token', response.data.token)\n\t\t\t\t\t\tthat.$router.push({\n\t\t\t\t\t\t\tpath: '/home'\n\t\t\t\t\t\t})\n\t\t\t\t\t})\n\t\t\t\t\t.catch(error => {\n\t\t\t\t\t\tconsole.log(error)\n\t\t\t\t\t})\n\t\t\t}\n\t\t}\n\t}\n</script>\n\n<style scoped>\n\t.out-div {\n\t\tposition: relative;\n\t}\n\n\t.in-card {\n\t\ttop: 50%;\n\t\tmargin: 150px auto;\n\t}\n\n\t.row {\n\t\tmargin-bottom: 20px;\n\t}\n</style>\n"]}]}