GAAA,IAAA,CAAA,IAAA;AACA,WAAA,IAAA,GAAA,IAAA,CAAA,IAAA;AACA,UAAA,KAAA,GAAA,KAAA,WAAA,CAAA,QAAA,CAAA,IAAA,CAAA;;AACA,WAAA,IAAA,CAAA,GAAA,CAAA,EAAA,CAAA,GAAA,KAAA,CAAA,MAAA,EAAA,CAAA,EAAA,EAAA;AACA,YAAA,IAAA,GAAA,EAAA;AACA,QAAA,IAAA,CAAA,MAAA,GAAA,KAAA,CAAA,CAAA,CAAA,CAAA,MAAA;;AACA,gBAAA,KAAA,CAAA,CAAA,CAAA,CAAA,MAAA;AACA,eAAA,CAAA;AAAA,YAAA,IAAA,CAAA,MAAA,GAAA,IAAA;AACA;;AACA,eAAA,CAAA;AAAA,YAAA,IAAA,CAAA,MAAA,GAAA,UAAA;AACA;;AACA,eAAA,CAAA;AAAA,YAAA,IAAA,CAAA,MAAA,GAAA,SAAA;AACA;;AACA,eAAA,CAAA;AAAA,YAAA,IAAA,CAAA,MAAA,GAAA,MAAA;AAPA;;AASA,QAAA,IAAA,CAAA,aAAA,GAAA,KAAA,CAAA,CAAA,CAAA,CAAA,aAAA;AACA,QAAA,IAAA,CAAA,UAAA,GAAA,KAAA,CAAA,CAAA,CAAA,CAAA,UAAA;AACA,QAAA,IAAA,CAAA,WAAA,GAAA,KAAA,CAAA,CAAA,CAAA,CAAA,WAAA;AACA,QAAA,IAAA,CAAA,WAAA,GAAA,KAAA,CAAA,CAAA,CAAA,CAAA,WAAA;AACA,aAAA,SAAA,CAAA,IAAA,CAAA,IAAA;AACA;AACA,KAjCA;AAkCA,IAAA,WAlCA,uBAkCA,IAlCA,EAkCA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,KAAA,CAAA,SAAA,CAAA,qBAAA,IAAA,CADA;;AAAA;AACA,gBAAA,QADA;AAEA,gBAAA,KAFA,GAEA,EAFA;AAGA,gBAAA,CAHA,GAGA,CAHA;;AAAA;AAAA,sBAGA,CAAA,GAAA,QAAA,CAAA,MAHA;AAAA;AAAA;AAAA;;AAAA;AAAA,uBAIA,KAAA,CAAA,SAAA,CAAA,0BAAA,QAAA,CAAA,CAAA,CAAA,CAJA;;AAAA;AAIA,gBAAA,IAJA;AAKA,gBAAA,KAAA,CAAA,IAAA,CAAA,IAAA;;AALA;AAGA,gBAAA,CAAA,EAHA;AAAA;AAAA;;AAAA;AAAA,iDAOA,KAPA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAQA,KA1CA;AA2CA,IAAA,QA3CA,oBA2CA,GA3CA,EA2CA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,KA7CA;AA8CA,IAAA,SA9CA,qBA8CA,GA9CA,EA8CA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA;AAhDA;AAVA,CAAA","sourcesContent":["<template>\n  <div>\n    <SearchUser @search-user-event=\"showData\"/>\n    <el-table\n        :data=\"tableData\"\n        border\n        style=\"margin-top: 30px\">\n      <el-table-column\n          prop=\"iouNum\"\n          label=\"借据号\">\n      </el-table-column>\n      <el-table-column\n          prop=\"customerCode\"\n          label=\"客户号\">\n        {{code}}\n      </el-table-column>\n      <el-table-column\n          prop=\"customerName\"\n          label=\"客户名\">\n        {{name}}\n      </el-table-column>\n      <el-table-column\n          prop=\"loanStatus\"\n          label=\"贷款状态\"\n          width=\"50\">\n      </el-table-column>\n      <el-table-column\n          prop=\"productName\"\n          label=\"贷款产品\">\n      </el-table-column>\n      <el-table-column\n          prop=\"productCode\"\n          label=\"贷款产品编号\">\n      </el-table-column>\n      <el-table-column\n          prop=\"overdueAmount\"\n          label=\"逾期金额(元)\">\n      </el-table-column>\n      <el-table-column\n          prop=\"createTime\"\n          label=\"发放日期\">\n      </el-table-column>\n      <el-table-column\n          prop=\"actions\"\n          label=\"操作\">\n        <template slot-scope=\"scope\">\n          <el-button @click=\"repayAll(scope.row)\" type=\"text\" size=\"small\">全部还款</el-button>\n          <el-button @click=\"repayPart(scope.row)\" type=\"text\" size=\"small\">部分还款</el-button>\n        </template>\n      </el-table-column>\n    </el-table>\n  </div>\n</template>\n\n<script>\nimport SearchUser from \"@/components/SearchUser\";\nexport default {\n  name: \"Index\",\n  components: {SearchUser},\n  data() {\n    return {\n      tableData: [],\n      code: \"\",\n      name: \"\",\n    }\n  },\n  methods: {\n    queryData(getPath) {\n      this.axios.get(getPath)\n          .then(response => {\n            return response.data\n          })\n          .catch(error => {\n            console.log(error)\n          })\n    },\n    showData(data) {\n      let customer = data\n      this.code = data.code\n      this.name = data.name\n      let loans = this.getLoanList(customer.code)\n      for(let i=0;i<loans.length;i++){\n        let temp = {}\n        temp.iouNum = loans[i].iouNum\n        switch (loans[i].status) {\n          case 0: temp.status = \"默认\"\n                break\n          case 1: temp.status = \"逾期，有罚金未还\"\n                break\n          case 2: temp.status = \"逾期，已还罚金\"\n                break\n          case 3: temp.status = \"正常还款\"\n        }\n        temp.overdueAmount = loans[i].overdueAmount\n        temp.createTime = loans[i].createTime\n        temp.productName = loans[i].productName\n        temp.productCode = loans[i].productCode\n        this.tableData.push(temp)\n      }\n    },\n    async getLoanList(code) {\n      let loanList = await this.queryData('/loan/list?code='+code)\n      let loans = []\n      for(let i=0;i<loanList.length;i++){\n        let loan = await this.queryData('/loan/product?iouNum='+loanList[i])\n        loans.push(loan)\n      }\n      return loans\n    },\n    repayAll(row) {\n      console.log(row)\n    },\n    repayPart(row) {\n      console.log(row)\n    },\n  }\n}\n</script>\n\n<style scoped>\n\n</style>\n"],"sourceRoot":"src/views/LoanRepayment"}]}